  object tClientes: TMSQuery
    Connection = Dados.Banco_Empresas
    SQL.Strings = (
      'SELECT DISTINCT  Clientes.Nome'
      '                ,Clientes.Ativo'
      '                ,Clientes.Contato'
      '                ,Clientes.Telefone1'
      '                ,Clientes.Rua'
      '                ,Clientes.Rua_Numero'
      '                ,Clientes.Bairro'
      '                ,Clientes.Municipio'
      '                ,Clientes.Estado'
      '                ,Clientes.CEP'
      
        '                ,(SELECT DISTINCT Fornecedores.Nome AS Nome_Trad' +
        'er FROM Fornecedores WHERE(Fornecedores.Codigo = Clientes.Trader' +
        ') ) AS Nome_Trader'
      '                ,Clientes.Email'
      'FROM   Clientes, Fornecedores'
      'ORDER BY Nome_Trader, Clientes.Nome')
    Active = True
    Left = 160
    Top = 7
  end
  object dstClientes: TDataSource
    DataSet = tClientes
