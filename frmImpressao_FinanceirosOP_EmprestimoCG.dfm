  object tEmprestimos: TMSQuery
    Connection = Dados.Banco_Empresas
    SQL.Strings = (
      '--SELECT * FROM Emprestimos'
      '--SELECT * FROM EmprestimosParcelas'
      ''
      
        'SELECT Banco = (SELECT Nome FROM Bancos WHERE Codigo = EM.Banco)' +
        ','
      '       EM.Garantia,'
      '       Emprestimo       = EM.Numero_Documento,'
      '       Data_Liberacao   = EM.Data,'
      
        '       Cliente          = (SELECT Nome FROM Clientes WHERE Codig' +
        'o = EM.Cliente),'
      
        '       Primeiro_Venc    = (SELECT MIN(Vencimento) FROM Emprestim' +
        'osParcelas EP WHERE EP.Emprestimo = EM.Numero),'
      
        '       Ultimo_Venc      = (SELECT MAX(Vencimento) FROM Emprestim' +
        'osParcelas EP WHERE EP.Emprestimo = EM.Numero),'
      
        '       Parcelas_Qtde    = (SELECT COUNT(*) FROM EmprestimosParce' +
        'las EP WHERE EP.Emprestimo = EM.Numero),'
      
        '       Parcelas_Pagas   = (SELECT COUNT(*) FROM PagarReceber    ' +
        '    PR WHERE Numero IN(SELECT Financeiro_Numero FROM Emprestimos' +
        'Parcelas EP WHERE EP.Emprestimo = EM.Numero) AND (SELECT COUNT(*' +
        ') FROM PagarReceberBaixas WHERE Numero IN(SELECT Financeiro_Nume' +
        'ro FROM EmprestimosParcelas EP WHERE EP.Emprestimo = EM.Numero))' +
        ' > 0),'
      
        '       Parcelas_Abertas = (SELECT COUNT(*) FROM EmprestimosParce' +
        'las EP WHERE EP.Emprestimo = EM.Numero) - (SELECT COUNT(*) FROM ' +
        'PagarReceber PR WHERE Numero IN(SELECT Financeiro_Numero FROM Em' +
        'prestimosParcelas EP WHERE EP.Emprestimo = EM.Numero) AND (SELEC' +
        'T COUNT(*) FROM PagarReceberBaixas WHERE Numero IN(SELECT Financ' +
        'eiro_Numero FROM EmprestimosParcelas EP WHERE EP.Emprestimo = EM' +
        '.Numero)) > 0),'
      '       Valor_Operacao   = ISNULL(Valor_Principal, 0),'
      
        '       Valor_Total      = ISNULL(Valor_Principal + Valor_Juros, ' +
        '0),'
      
        '       Valor_Pago       = ISNULL((SELECT SUM(Valor) FROM PagarRe' +
        'ceberBaixas WHERE Numero IN(SELECT Financeiro_Numero FROM Empres' +
        'timosParcelas EP WHERE EP.Emprestimo = EM.Numero)), 0),'
      
        '       Valor_Aberto     = ISNULL((Valor_Principal + Valor_Juros)' +
        ' - (SELECT SUM(Valor) FROM PagarReceberBaixas WHERE Numero IN(SE' +
        'LECT Financeiro_Numero FROM EmprestimosParcelas EP WHERE EP.Empr' +
        'estimo = EM.Numero)), 0),'
      
        '       Valor_IOF_Taxa   = ISNULL(Valor_IOF + Valor_Diversos , 0)' +
        ','
      '       Valor_Aplicacao  = ISNULL(Valor_Aplicacao, 0),'
      
        '       Valor_Custo      = ISNULL((Valor_Principal - (Valor_Princ' +
        'ipal + Valor_Juros)) + (Valor_IOF + Valor_Diversos), 0),'
      '       Liquidada        = '#39#39','
      '       Origem           = '#39'MT'#39
      'INTO   #TEMP'
      'FROM   Emprestimos EM'
      'WHERE  Tipo = '#39'CG'#39
      ''
      
        'UPDATE #TEMP SET Liquidada = CASE WHEN Valor_Aberto = 0 THEN CHA' +
        'R(149) ELSE '#39#39' END'
      ''
      'SELECT * FROM #TEMP'
      'ORDER BY Emprestimo'
      ''
      'DROP TABLE #TEMP')
    Left = 280
    Top = 8
  end
