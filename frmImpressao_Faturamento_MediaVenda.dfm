  object tItens: TMSQuery
    Connection = Dados.Banco_Empresas
    SQL.Strings = (
      
        'SELECT (SELECT Linha FROM Produtos WHERE(Codigo = Codigo_Mercado' +
        'ria)) AS Linha,'
      '       Codigo_Mercadoria,'
      '       Valor_Unitario,'
      '       Valor_Total,'
      '       Quantidade'
      'INTO  #Temp'
      'FROM  NotasItens'
      
        'WHERE(Saida_Entrada = 1) AND (Cancelada <> 1) AND (Data BETWEEN ' +
        #39'05/01/2011'#39' AND '#39'05/31/2011'#39')'
      
        '--      AND (SELECT Linha FROM Produtos WHERE(Codigo = Codigo_Me' +
        'rcadoria)) = 3'
      ''
      'SELECT Linha,'
      '       Codigo_Mercadoria,'
      '       SUM(Quantidade) AS Quantidade,'
      '       MIN(Valor_Total/Quantidade) AS Valor_Menor,'
      '       SUM(Valor_Total)/SUM(Quantidade) AS Valor_Medio,'
      '       MAX(Valor_Total/Quantidade) AS Valor_Maior'
      'INTO   #Temp2'
      'FROM   #Temp'
      'WHERE  Quantidade > 0'
      'GROUP  BY Linha, Codigo_Mercadoria'
      ''
      'SELECT #Temp2.*, '
      
        '      (SELECT Unidade FROM Produtos WHERE(Codigo = #Temp2.Codigo' +
        '_Mercadoria)) AS Unidade,'
      
        '      (SELECT SUBSTRING(Descricao, 1, LEN(LTRIM(RTRIM(CAST(Descr' +
        'icao AS VARCHAR(500)))))-(4+LEN(Codigo)) ) FROM Produtos WHERE (' +
        'Codigo = #Temp2.Codigo_Mercadoria)) AS Descricao,'
      
        '      (SELECT Descricao FROM ProdutosLinhas WHERE (Codigo = #Tem' +
        'p2.Linha)) AS Linha_Descricao '
      'FROM   #Temp2'
      'ORDER BY Linha, Codigo_Mercadoria'
      'DROP TABLE #Temp'
      'DROP TABLE #Temp2')
    FetchRows = 1
    RefreshOptions = [roAfterInsert, roAfterUpdate, roBeforeEdit]
    Active = True
    Left = 288
    Top = 8
  end
