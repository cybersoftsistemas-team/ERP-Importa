  object tImpressao: TMSQuery
    SQLUpdate.Strings = (
      'UPDATE ImpressaoServico'
      'SET'
      
        '  Reg = :Reg, Tabela = :Tabela, Campo = :Campo, Pesquisa = :Pesq' +
        'uisa, Referencia = :Referencia, Linha = :Linha, Coluna = :Coluna' +
        ', Tamanho = :Tamanho, Tipo = :Tipo, Mascara = :Mascara, Descrica' +
        'o = :Descricao, Imprimir = :Imprimir, Valor_Padrao = :Valor_Padr' +
        'ao'
      'WHERE'
      '  Reg = :Old_Reg')
    SQLRefresh.Strings = (
      
        'SELECT ImpressaoServico.Reg, ImpressaoServico.Banco, ImpressaoSe' +
        'rvico.Tabela, ImpressaoServico.Campo, ImpressaoServico.Pesquisa,' +
        ' ImpressaoServico.Referencia, ImpressaoServico.Linha, ImpressaoS' +
        'ervico.Coluna, ImpressaoServico.Tamanho, ImpressaoServico.Tipo, ' +
        'ImpressaoServico.Mascara, ImpressaoServico.Descricao, ImpressaoS' +
        'ervico.Imprimir, ImpressaoServico.Valor_Padrao FROM ImpressaoSer' +
        'vico'
      'WHERE ImpressaoServico.Reg = :Reg ')
    Connection = Dados.Banco
    FetchRows = 1
    RefreshOptions = [roAfterInsert, roAfterUpdate, roBeforeEdit]
    Left = 423
    Top = 3
  end
