    Top = 80
  end
  object tCarta: TMSQuery
    Connection = Dados.Banco_Empresas
    SQL.Strings = (
      'SELECT *,'
      
        '       (SELECT Saida_Entrada FROM NotasFiscais NF WHERE(NF.Numer' +
        'o = Nota AND NF.Data_Emissao = CartaCorrecao.Data_Emissao)) AS S' +
        'aida_Entrada,'
      
        '       (SELECT Serie FROM NotasFiscais NF WHERE(NF.Numero = Nota' +
        ' AND NF.Data_Emissao = CartaCorrecao.Data_Emissao)) AS Serie,'
      
        '       (SELECT Processo FROM NotasFiscais NF WHERE(NF.Numero = N' +
        'ota AND NF.Data_Emissao = CartaCorrecao.Data_Emissao)) AS Proces' +
        'so'
      'FROM CartaCorrecao')
    FetchRows = 1
    DetailFields = 'Nota;Data'
    Active = True
    Left = 186
    Top = 128
  end
  object dstCarta: TDataSource
    DataSet = tCarta
