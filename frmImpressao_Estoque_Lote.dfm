  object tLotes: TMSQuery
    Connection = Dados.Banco_Empresas
    SQL.Strings = (
      '-- ENTRADAS.'
      'USE Cybersoft_DIAGMASTER'
      'SELECT Produto      = Produto_Codigo'
        '      ,Codigo_Fab   = (SELECT Codigo_Fabricante FROM Produtos WH' +
        'ERE Codigo = Produto_Codigo)'
        '      ,Produto_Desc = (SELECT CAST(Descricao AS VARCHAR(250)) FR' +
        'OM Produtos WHERE Codigo = Produto_Codigo)'
      '      ,Lote'
      '      ,Destinatario = '#39'TESTE'#39
      '      ,Nota_Entra   = Nota_Entrada'
      '      ,Data_Entra   = CONVERT(VARCHAR(12), Data_Entrada,103)'
      '      ,Qtde_Entra   = Quantidade_Entrada'
      '      ,Nota_Saida   = null'
      '      ,Data_Saida   = null'
      '      ,Qtde_Saida   = 0'
      '      ,Data_Val     = CONVERT(VARCHAR(12), Data_Validade,103)'
      '      ,Processo'
      '      ,Saida_Entrada = 0'
      'FROM ProdutosDetalhe'
      ''
        '/* ATEN'#199#195'O!  Substituir todas as linhas que contenham Pedidos e ' +
        'PedidosItens por Notas e NotasItens */'
      ''
      '--SA'#205'DAS.'
      'UNION ALL'
      'SELECT Produto      = Produto_Codigo'
        '      ,Codigo_Fab   = (SELECT Codigo_Fabricante FROM Produtos WH' +
        'ERE Codigo = Produto_Codigo)'
        '      ,Produto_Desc = (SELECT CAST(Descricao AS VARCHAR(250)) FR' +
        'OM Produtos WHERE Codigo = Produto_Codigo)'
      '      ,Lote'
      '      ,Destinatario = '#39'TESTE'#39
      '      ,Nota_Entra   = null'
      '      ,Data_Entra   = null'
      '      ,Qtde_Entra   = 0'
      '      ,Nota_Saida   = Pedido'
        '      ,Data_Saida   = CONVERT(VARCHAR(12), GETDATE(),103)    -- ' +
        'ATEN'#199#195'O! Substiruir pela data da nota de saida.'
      '      ,Qtde_Saida   = Quantidade'
      '      ,Data_Val     = null'
        '      ,Processo     = (SELECT DISTINCT Processo FROM PedidosIten' +
        's PI WHERE PI.Pedido = PID.Pedido AND PI.Codigo_Mercadoria = PID' +
        '.Produto_Codigo)'
      '      ,Saida_Entrada'
      '--FROM  NotasItens          '
      'FROM  PedidosItensDetalhe PID     '
      'WHERE Saida_Entrada= 1'
      'ORDER BY Produto,Lote,Saida_Entrada,Data_Val, Data_Entra')
    FetchRows = 1
    Active = True
    Left = 328
    Top = 8
  end
