  object tLancamentos: TMSQuery
    SQLInsert.Strings = (
      
        'INSERT INTO Cybersoft_Contabilidade_ABRASIL_SANTACATARINA..Lanca' +
        'mentos'
      
        '  (Data, Numero, Conta_Debito, Conta_Credito, Historico_Codigo, ' +
        'Valor)'
      'VALUES'
      
        '  (:Data, :Numero, :Conta_Debito, :Conta_Credito, :Historico_Cod' +
        'igo, :Valor)')
    SQLDelete.Strings = (
      
        'DELETE FROM Cybersoft_Contabilidade_ABRASIL_SANTACATARINA..Lanca' +
        'mentos'
      'WHERE'
      '  Data = :Old_Data AND Numero = :Old_Numero')
    SQLUpdate.Strings = (
      
        'UPDATE Cybersoft_Contabilidade_ABRASIL_SANTACATARINA..Lancamento' +
        's'
      'SET'
      
        '  Data = :Data, Numero = :Numero, Conta_Debito = :Conta_Debito, ' +
        'Conta_Credito = :Conta_Credito, Historico_Codigo = :Historico_Co' +
        'digo, Valor = :Valor'
      'WHERE'
      '  Data = :Old_Data AND Numero = :Old_Numero')
    SQLRefresh.Strings = (
      
        'SELECT Cybersoft_Contabilidade_ABRASIL_SANTACATARINA..Lancamento' +
        's.Data, Cybersoft_Contabilidade_ABRASIL_SANTACATARINA..Lancament' +
        'os.Numero, Cybersoft_Contabilidade_ABRASIL_SANTACATARINA..Lancam' +
        'entos.Conta_Debito, Cybersoft_Contabilidade_ABRASIL_SANTACATARIN' +
        'A..Lancamentos.Conta_Credito, Cybersoft_Contabilidade_ABRASIL_SA' +
        'NTACATARINA..Lancamentos.Historico_Codigo, Cybersoft_Contabilida' +
        'de_ABRASIL_SANTACATARINA..Lancamentos.Valor FROM Cybersoft_Conta' +
        'bilidade_ABRASIL_SANTACATARINA..Lancamentos'
      
        'WHERE Cybersoft_Contabilidade_ABRASIL_SANTACATARINA..Lancamentos' +
        '.Data = :Data AND Cybersoft_Contabilidade_ABRASIL_SANTACATARINA.' +
        '.Lancamentos.Numero = :Numero ')
    Connection = Dados.Banco_Contabilidade
    SQL.Strings = (
      'SELECT Data,'
      '       Lote,'
      '       Numero,'
      '       Conta_Debito,'
      '       Conta_Credito,'
      '       Historico_Codigo,'
      
        '       LTRIM(RTRIM(ISNULL((SELECT ISNULL(Descricao, '#39#39') FROM His' +
        'toricoPadrao WHERE(Codigo = Lancamentos.Historico_Codigo)),'#39#39')))' +
        ' + '#39' '#39'+ISNULL(CAST(Historico_Complemento AS VARCHAR(100)), '#39#39') C' +
        'OLLATE SQL_Latin1_General_CP1_CI_AS AS Historico_Descricao,'
      '       Valor,'
      '       D_C = '#39'D'#39','
      '       Saldo = CAST(0 AS money),'
      '       Linha = cast(0 as bigint),'
      '       Beneficiario,'
      '       Processo_Debito,'
      '       Processo_Credito'
      'FROM   Lancamentos'
      'ORDER BY Data, Numero')
    FetchRows = 1
    Left = 376
    Top = 224
