  object tBalancete: TMSQuery
    Connection = Dados.Banco_Contabilidade
    SQL.Strings = (
      '-- PLANO NORMAL'
      'SELECT PC.Conta'
      '      ,PC.Codigo'
      '      ,PC.Descricao'
      '      ,PC.Sintetica'
      '      ,PC.Titulo'
      '      ,PC.Saldo_Anterior'
      '      ,PC.Tipo_Terceiros'
      '      ,PC.Codigo_Terceiros'
      '      ,PC.Natureza'
      '      ,Atualiza_Anterior = CASE WHEN PC.Natureza = '#39'C'#39' THEN'
      '                                ISNULL(PC.Saldo_Anterior, 0) -'
      
        '                                ISNULL((SELECT SUM(Valor) FROM L' +
        'ancamentos LAN WHERE LAN.Empresa = '#39'35825513000116'#39' AND PC.Sinte' +
        'tica = 0 AND LAN.Conta_Debito  = PC.Codigo AND LAN.Data < '#39'09/01' +
        '/2020'#39'), 0) +'
      
        '                                ISNULL((SELECT SUM(Valor) FROM L' +
        'ancamentos LAN WHERE LAN.Empresa = '#39'35825513000116'#39' AND PC.Sinte' +
        'tica = 0 AND LAN.Conta_Credito = PC.Codigo AND LAN.Data < '#39'09/01' +
        '/2020'#39'), 0)'
      '                           ELSE'
      '                                ISNULL(PC.Saldo_Anterior, 0) +'
      
        '                                ISNULL((SELECT SUM(Valor) FROM L' +
        'ancamentos LAN WHERE LAN.Empresa = '#39'35825513000116'#39' AND PC.Sinte' +
        'tica = 0 AND LAN.Conta_Debito  = PC.Codigo AND LAN.Data < '#39'09/01' +
        '/2020'#39'), 0) -'
      
        '                                ISNULL((SELECT SUM(Valor) FROM L' +
        'ancamentos LAN WHERE LAN.Empresa = '#39'35825513000116'#39' AND PC.Sinte' +
        'tica = 0 AND LAN.Conta_Credito = PC.Codigo AND LAN.Data < '#39'09/01' +
        '/2020'#39'), 0)'
      '                           END'
      
        '      ,Atualiza_Debito   = ISNULL((SELECT SUM(Valor) FROM Lancam' +
        'entos LAN WHERE LAN.Empresa = '#39'35825513000116'#39' AND PC.Sintetica ' +
        '= 0 AND LAN.Conta_Debito  = PC.Codigo AND LAN.Data < '#39'09/01/2020' +
        #39'), 0)'
      
        '      ,Atualiza_Credito  = ISNULL((SELECT SUM(Valor) FROM Lancam' +
        'entos LAN WHERE LAN.Empresa = '#39'35825513000116'#39' AND PC.Sintetica ' +
        '= 0 AND LAN.Conta_Credito = PC.Codigo AND LAN.Data < '#39'09/01/2020' +
        #39'), 0)'
      
        '      ,Debito            = ISNULL((SELECT SUM(Valor) FROM Lancam' +
        'entos LAN WHERE LAN.Empresa = '#39'35825513000116'#39' AND PC.Sintetica ' +
        '= 0 AND LAN.Conta_Debito  = PC.Codigo AND LAN.Data BETWEEN '#39'09/0' +
        '1/2020'#39' AND '#39'09/30/2020'#39'), 0)'
      
        '      ,Credito           = ISNULL((SELECT SUM(Valor) FROM Lancam' +
        'entos LAN WHERE LAN.Empresa = '#39'35825513000116'#39' AND PC.Sintetica ' +
        '= 0 AND LAN.Conta_Credito = PC.Codigo AND LAN.Data BETWEEN '#39'09/0' +
        '1/2020'#39' AND '#39'09/30/2020'#39'), 0)'
      '      ,Saldo_Atual       = CAST(0 AS money)'
      '      ,DC_Anterior       = '#39' '#39' '
      '      ,DC_Atual          = '#39' '#39' '
      'INTO   #PlanoTmp'
      'FROM   PlanoContas PC'
      'WHERE  Conta IS NOT NULL'
      '-- TOTALIZANDO CONTAS TITULO'
      
        'UPDATE #PlanoTmp SET Debito            = ISNULL((SELECT SUM(Debi' +
        'to           ) FROM #PlanoTmp PT WHERE PT.Conta LIKE RTRIM(#Plan' +
        'oTmp.Conta)+'#39'%'#39' AND PT.Sintetica = 0), 0)'
      
        '                    ,Credito           = ISNULL((SELECT SUM(Cred' +
        'ito          ) FROM #PlanoTmp PT WHERE PT.Conta LIKE RTRIM(#Plan' +
        'oTmp.Conta)+'#39'%'#39' AND PT.Sintetica = 0), 0)'
      
        '                    ,Atualiza_Anterior = ISNULL((SELECT SUM(Atua' +
        'liza_Anterior) FROM #PlanoTmp PT WHERE PT.Conta LIKE RTRIM(#Plan' +
        'oTmp.Conta)+'#39'%'#39' AND PT.Sintetica = 0), 0)'
      'WHERE(#PlanoTmp.Sintetica = 1)'
      '-- APURANDO SALDO ATUAL --'
      
        'UPDATE #PlanoTmp SET Saldo_Atual = CASE WHEN #PlanoTmp.Natureza ' +
        '= '#39'C'#39' THEN'
      
        '                                        (Atualiza_Anterior - Deb' +
        'ito + Credito)'
      '                                   ELSE'
      
        '                                        (Atualiza_Anterior + Deb' +
        'ito - Credito)'
      '                                   END'
      '-- AJUSTANDO SIMBOLO "D'#201'BITO" e "CR'#201'DITO" --'
      'UPDATE #PlanoTmp SET DC_Anterior = CASE WHEN Natureza = '#39'D'#39' THEN'
      
        '                                        CASE WHEN Atualiza_Anter' +
        'ior >= 0 THEN '#39'D'#39' ELSE '#39'C'#39' END'
      '                                   ELSE'
      
        '                                        CASE WHEN Atualiza_Anter' +
        'ior >= 0 THEN '#39'C'#39' ELSE '#39'D'#39' END'
      '                                   END'
      '                    ,DC_Atual    = CASE WHEN Natureza = '#39'D'#39' THEN'
      
        '                                        CASE WHEN Saldo_Atual >=' +
        ' 0 THEN '#39'D'#39' ELSE '#39'C'#39' END'
      '                                   ELSE'
      
        '                                        CASE WHEN Saldo_Atual >=' +
        ' 0 THEN '#39'C'#39' ELSE '#39'D'#39' END'
      '                                   END'
      'SELECT Conta'
      '      ,Codigo'
      '      ,Descricao'
      '      ,Sintetica'
      '      ,Titulo'
      '      ,Tipo_Terceiros'
      '      ,Natureza'
      
        '      ,Saldo_Anterior = CASE WHEN Atualiza_Anterior >= 0 THEN At' +
        'ualiza_Anterior ELSE Atualiza_Anterior *-1 END'
      '      ,DC_Anterior'
      
        '      ,Debito         = CASE WHEN Debito  >= 0 THEN Debito  ELSE' +
        ' Debito  *-1 END'
      
        '      ,Credito        = CASE WHEN Credito >= 0 THEN Credito ELSE' +
        ' Credito *-1 END'
      
        '      ,Saldo_Atual    = CASE WHEN Saldo_Atual >= 0 THEN Saldo_At' +
        'ual ELSE Saldo_Atual *-1 END'
      '      ,Grupo = SUBSTRING(Conta,1,1)'
      '      ,DC_Atual'
      'FROM   #PlanoTmp'
      'WHERE  Conta IS NOT NULL'
      'ORDER BY Conta, Sintetica desc'
      'DROP TABLE #PlanoTmp')
    AfterScroll = tBalanceteAfterScroll
    Left = 288
    Top = 224
