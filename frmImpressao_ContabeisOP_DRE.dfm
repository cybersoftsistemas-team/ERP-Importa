  object tDRE: TMSQuery
    Connection = Dados.Banco_Contabilidade
    SQL.Strings = (
      '-- PLANO NORMAL --'
      'SELECT PC.Conta'
      '      ,PC.Codigo'
      '      ,PC.Nivel'
      '      ,PC.Descricao'
      '      ,PC.Sintetica'
      '      ,PC.Saldo_Anterior'
      '      ,PC.Tipo_Terceiros'
      '      ,PC.Codigo_Terceiros'
      '      ,PC.Natureza'
      '      ,Atualiza_Anterior = CASE WHEN PC.Natureza = '#39'C'#39' THEN'
      '                                ISNULL(PC.Saldo_Anterior, 0) -'
      
        '                                ISNULL((SELECT SUM(Valor) FROM L' +
        'ancamentos LAN WHERE PC.Sintetica = 0 AND LAN.Conta_Debito  = PC' +
        '.Codigo AND LAN.Data < '#39'01/01/2016'#39'), 0) +'
      
        '                                ISNULL((SELECT SUM(Valor) FROM L' +
        'ancamentos LAN WHERE PC.Sintetica = 0 AND LAN.Conta_Credito = PC' +
        '.Codigo AND LAN.Data < '#39'01/01/2016'#39'), 0)'
      '                           ELSE'
      '                                ISNULL(PC.Saldo_Anterior, 0) +'
      
        '                                ISNULL((SELECT SUM(Valor) FROM L' +
        'ancamentos LAN WHERE PC.Sintetica = 0 AND LAN.Conta_Debito  = PC' +
        '.Codigo AND LAN.Data < '#39'01/01/2016'#39'), 0) -'
      
        '                                ISNULL((SELECT SUM(Valor) FROM L' +
        'ancamentos LAN WHERE PC.Sintetica = 0 AND LAN.Conta_Credito = PC' +
        '.Codigo AND LAN.Data < '#39'01/01/2016'#39'), 0)'
      '                           END'
      
        '      ,Atualiza_Debito   = ISNULL((SELECT SUM(Valor) FROM Lancam' +
        'entos LAN WHERE PC.Sintetica = 0 AND LAN.Conta_Debito  = PC.Codi' +
        'go AND LAN.Data < '#39'01/01/2016'#39'), 0)'
      
        '      ,Atualiza_Credito  = ISNULL((SELECT SUM(Valor) FROM Lancam' +
        'entos LAN WHERE PC.Sintetica = 0 AND LAN.Conta_Credito = PC.Codi' +
        'go AND LAN.Data < '#39'01/01/2016'#39'), 0)'
      
        '      ,Debito            = ISNULL((SELECT SUM(Valor) FROM Lancam' +
        'entos LAN WHERE PC.Sintetica = 0 AND LAN.Conta_Debito  = PC.Codi' +
        'go AND LAN.Data BETWEEN '#39'01/01/2016'#39' AND '#39'31/12/2016'#39'), 0)'
      
        '      ,Credito           = ISNULL((SELECT SUM(Valor) FROM Lancam' +
        'entos LAN WHERE PC.Sintetica = 0 AND LAN.Conta_Credito = PC.Codi' +
        'go AND LAN.Data BETWEEN '#39'01/01/2016'#39' AND '#39'31/12/2016'#39'), 0)'
      '      ,Saldo_Atual       = CAST(0 AS money)'
      '      ,DC_Anterior       = '#39' '#39' '
      '      ,DC_Atual          = '#39' '#39' '
      '      ,DRE'
      'INTO   #PlanoTmp'
      'FROM   PlanoContas PC'
      'WHERE  Conta IS NOT NULL'
      '-- TOTALIZANDO CONTAS TITULO'
      
        'UPDATE #PlanoTmp SET Debito            = ISNULL((SELECT SUM(Debi' +
        'to)            FROM #PlanoTmp PT WHERE PT.Conta LIKE RTRIM(#Plan' +
        'oTmp.Conta)+'#39'%'#39' AND PT.Sintetica = 0), 0)'
      
        '                    ,Credito           = ISNULL((SELECT SUM(Cred' +
        'ito)           FROM #PlanoTmp PT WHERE PT.Conta LIKE RTRIM(#Plan' +
        'oTmp.Conta)+'#39'%'#39' AND PT.Sintetica = 0), 0)'
      
        '                    ,Atualiza_Anterior = ISNULL((SELECT SUM(Atua' +
        'liza_Anterior) FROM #PlanoTmp PT WHERE PT.Conta LIKE RTRIM(#Plan' +
        'oTmp.Conta)+'#39'%'#39' AND PT.Sintetica = 0), 0)'
      'WHERE(#PlanoTmp.Sintetica = 1)'
      '-- APURANDO SALDO ATUAL --'
      
        'UPDATE #PlanoTmp SET Saldo_Atual = CASE WHEN #PlanoTmp.Natureza ' +
        '= '#39'C'#39' THEN'
      
        '                                        CAST((Atualiza_Anterior ' +
        '- Debito + Credito) AS DECIMAL(14,2))'
      '                                   ELSE'
      
        '                                        CAST((Atualiza_Anterior ' +
        '+ Debito - Credito) AS DECIMAL(14,2))'
      '                                   END'
      '-- AJUSTANDO SIMBOLO "D'#201'BITO" e "CR'#201'DITO" --'
      'UPDATE #PlanoTmp SET DC_Anterior = CASE WHEN Natureza = '#39'D'#39' THEN'
      
        '                                        CASE WHEN Atualiza_Anter' +
        'ior >= 0 THEN '#39'D'#39' ELSE '#39'C'#39' END'
      '                                   ELSE'
      
        '                                        CASE WHEN Atualiza_Anter' +
        'ior >= 0 THEN '#39'C'#39' ELSE '#39'D'#39' END'
      '                                   END'
      '                    ,DC_Atual    = CASE WHEN Natureza = '#39'D'#39' THEN'
      
        '                                        CASE WHEN Saldo_Atual >=' +
        ' 0 THEN '#39'D'#39' ELSE '#39'C'#39' END'
      '                                   ELSE'
      
        '                                        CASE WHEN Saldo_Atual >=' +
        ' 0 THEN '#39'C'#39' ELSE '#39'D'#39' END'
      '                                   END'
      ''
      ''
      '-- MONTANDO A DRE'
      'SELECT Codigo '
      '      ,Descricao'
      
        '      ,Saldo_Atual = (SELECT SUM(Saldo_Atual) FROM #PlanoTmp WHE' +
        'RE #PlanoTmp.DRE = DCG.Codigo)'
      #9'  ,Totalizador'
      #9'  ,Calculo'
      'INTO #DRETemp'
      'FROM   DREContabilGrupos DCG'
      ''
      
        'INSERT INTO #DRETemp VALUES(1000, '#39'RECEITAS OPERACIONAIS L'#205'QUIDA' +
        'S'#39'       , 0, 1,'#39'='#39')'
      
        'INSERT INTO #DRETemp VALUES(1001, '#39'LUCRO BRUTO / RESULTADO BRUTO' +
        #39'        , 0, 2,'#39'='#39')'
      
        'INSERT INTO #DRETemp VALUES(1002, '#39'LUCRO OU PREJU'#205'ZO OPERACIONAL' +
        #39'        , 0, 3,'#39'='#39')'
      
        'INSERT INTO #DRETemp VALUES(1003, '#39'RESULTADO DO EXERC'#205'CIO ANTES ' +
        'DO I.R.'#39' , 0, 4,'#39'='#39')'
      
        'INSERT INTO #DRETemp VALUES(1004, '#39'RESULTADO DO EXERC'#205'CIO DEPOIS' +
        ' DO I.R.'#39', 0, 5,'#39'='#39')'
      ''
      
        'UPDATE #DRETemp SET Saldo_Atual = (SELECT SUM(CASE WHEN Calculo ' +
        '= '#39'+'#39' THEN Saldo_Atual ELSE Saldo_Atual *-1 END) FROM #DRETemp T' +
        'mp2 WHERE Tmp2.Totalizador = #DRETemp.Totalizador) '
      'WHERE Codigo >= 1000'
      ''
      
        'UPDATE #DRETemp SET Saldo_Atual = (SELECT SUM(Saldo_Atual) FROM ' +
        '#DRETemp Tmp2 WHERE Tmp2.Codigo > 100 AND Tmp2.Codigo <= #DRETem' +
        'p.Codigo) '
      'WHERE Codigo > 1000'
      ''
      ''
      'SELECT * FROM #DRETemp '
      'ORDER BY Totalizador, Codigo'
      ''
      'DROP TABLE #PlanoTmp, #DRETemp')
    FetchRows = 1
    Left = 312
    Top = 56
  end
  object dstDRE: TDataSource
    DataSet = tDRE
    Left = 344
    Top = 56
  end
