  object tEmpresas: TMSQuery
    Connection = Dados.Banco
    SQL.Strings = (
      'SELECT Codigo, '
      
        '       SUBSTRING(CNPJ,1,2)+'#39'.'#39'+SUBSTRING(CNPJ,3,3)+'#39'.'#39'+SUBSTRING' +
        '(CNPJ,6,3)+'#39'/'#39'+SUBSTRING(CNPJ,9,4)+'#39'-'#39'+SUBSTRING(CNPJ,13,2) AS C' +
        'NPJ, '
      '       Razao_Social, '
      '       Numero_Filial, '
      '       Estado '
      'FROM Empresas ORDER BY CNPJ')
    FetchRows = 1
    Left = 376
    Top = 56
